<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.oneconnect.leadership.subscriber">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".SubscriberApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_beacon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <service android:name=".services.SubscriberMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".SubscriberSignInActivityBase"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NotifHandlerActivity"
            android:label="@string/title_activity_notif_handler"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SubscriberMainActivity"
            android:label="@string/title_activity_subscriber_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name="com.oneconnect.leadership.library.lists.MediaListActivity"
            android:parentActivityName=".SubscriberMainActivity"
            android:label="@string/title_activity_media_list"/>

        <activity android:name="com.oneconnect.leadership.library.activities.DailyThoughtActivity"
        android:label="@string/title_activity_daily_thought"
        android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.activities.MasterActivity"
            android:label="@string/title_activity_weekly_classes"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.activities.eBookActivity"
            android:label="@string/title_activity_e_book"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.activities.VideoActivity"
            android:label="@string/title_activity_video"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.activities.PodcastActivity"
            android:label="@string/title_activity_podcast"
            android:parentActivityName=".SubscriberMainActivity"/>
        <activity
            android:name="com.oneconnect.leadership.library.activities.eBookWebActivity"
            android:label="@string/ebook_web_activity"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.oneconnect.leadership.library.activities.WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.video.LeExoPlayerActivity"
            android:label="@string/title_activity_le_exo_player"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.video.VideoPlayerActivity"
            android:label="@string/title_activity_video_player"
            android:parentActivityName=".SubscriberMainActivity"/>

        <activity android:name="com.oneconnect.leadership.library.activities.PhotoActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".SubscriberMainActivity"/>



    </application>

</manifest>